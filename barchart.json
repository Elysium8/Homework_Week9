{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bar chart that sorts the y-values by the x-values.",
    "data": {"url": "Speedinfo.csv"},
    "transform": [{"filter": "datum.Step > 200"}],
    "height": {"step": 17},
    "mark": "bar",
    "encoding": {
      "y": {
        "field": "Country",
        "type": "nominal",
        "sort": "-x"
      },
      "x": {
        "aggregate": "sum",
        "field": "Step",
        "title": "Speed"
      }
    }
  }